
<div class="banner" style="background-image: url(https://img.freepik.com/free-psd/3d-cinema-blank-banner-background_23-2150822410.jpg?t=st=1746785911~exp=1746789511~hmac=28fb143ce1cded19a6827f9ebeec32b8302c5b3497ca8fcfaf69ab45a9d035d8&w=2000);">
  <div class="container position-relative">
    <h1>My Lists</h1>
  </div>
  <div class="position-absolute top-0 end-0">
    <%= link_to "Create new List", new_list_path, class:"btn btn-primary"%>
  </div>
</div>



<% @lists.each do |list|%>
  <div class="card" style="background-image: url('<%= cl_image_path list.photo.key, height: 300, crop: :fill %>')">
    <h5 class="card-title"><%= link_to list.name, list_path(list) %></h5>
  </div>
<% end %>
